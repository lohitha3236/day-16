#include<stdio.h>
int main()
{
int num,i,evensum=0,oddsum=0;
printf("enter a number");
scanf("%d",&num);
printf("numberes divisible by %d:\n",num);
for (i=1;i<=20;i++)
{
printf("%d ",num==0);
{
printf("%d\n",i);
if(i%2==0)
{
evensum+=i;
}
else
{
oddsum+=i;
}
}
}
printf("evensum=%d\n",evensum);
printf("oddsum=%d\n",oddsum);
return 0;
}
